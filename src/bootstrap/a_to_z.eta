import type { PlangsGraph } from '<%= '../'.repeat(it.depth) %>entities/plangs_graph';

export function define(g: PlangsGraph) {
   const lb = g.<%= it.builderName %>Builder;

<% for (const bundle of it.data) { %>
  lb.define(<%= bundle.vid %><% if (bundle.data) { %>,<%= bundle.data %><% } %><% if (bundle.data && bundle.vrelations) { %>, <%= bundle.vrelations %> <% } %>);

<% } %>
}
