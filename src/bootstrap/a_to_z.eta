import type { PlangsGraph } from '../../entities/plangs_graph';

export function define(g: PlangsGraph) {
   const lb = g.<%= it.builderName %>Builder;

<% for (const bundle of it.data) { %>
  //------------------------------------------------------------------------------- 

  lb.define(
    <%= bundle.vid %>,
    <%= bundle.name %>,
    <%= bundle.data %><% if (bundle.vrelations) { %>,
    <%= bundle.vrelations %>
    <% } %>);

<% } %>
}
